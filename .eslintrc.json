{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "jasmine": true,
    "node": true
  },  
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "settings": {
    "ecmascript": 6,
    "jsx": true
  },  
  "plugins": [
    "react"
    ],  
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "semi": [2, "always"],
      "react/prop-types": 0,
      "no-console": 0
    } 
}
